                    "type" : "git",
                    "url" : "https://github.com/qgis/QGIS.git",
                    "branch" : "final-3_12_3"
                }
            ],
            "config-opts" : [
                "-DWITH_3D=TRUE",
                "-DGRASS_PREFIX7=/app/grass76",
                "-DQWT_LIBRARY=/app/lib/libqwt.so"
            ],
            "modules" : [
